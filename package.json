{
  "name": "zenn-viewer",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "prepare": "husky install",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "dev": "react-devtools",
    "web": "expo start --web",
    "check": "biome check --apply ./src",
    "format": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "lint": "biome lint ./src",
    "lint:fix": "biome lint --apply ./src"
  },
  "dependencies": {
    "@shopify/flash-list": "1.6.3",
    "expo": "^50.0.0",
    "expo-constants": "~15.4.5",
    "expo-image": "~1.10.4",
    "expo-linking": "~6.2.2",
    "expo-router": "~3.4.3",
    "expo-status-bar": "~1.11.1",
    "expo-web-browser": "~12.8.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-super-grid": "^5.0.0",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@biomejs/biome": "1.5.2",
    "@types/react": "~18.2.14",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "react-devtools": "^5.0.0",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": ["biome check --apply --no-errors-on-unmatched"]
  },
  "private": true
}
